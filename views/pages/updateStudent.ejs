<link rel="stylesheet" href="/styles/app.css">
      <script type="text/javascript">
      setTimeout(function sunrise () {
        document.getElementsByClassName('header')[0].style.backgroundColor = '#118798';
      }, 0);
      </script>
      
      <div class="default-page">
        <div class="header">
          <h1 id="main-title" class="container">Student ERP portal.</h1>
          <h3 class="container">You're looking at: <code>Update Student Data</code></h3>
        </div>
        <div class="container">
          <br>
          <h2>All Registered Students</h2><br>
          <p>CRUD Operations</p>   <br><br>         
          <table class="table table-hover">
            <thead>
             
              <tr>
                <th>ID </th>
                <th>Name</th>
                <th>Update Data</th>
               
              </tr>
            </thead>
            <tbody>
                <% students.forEach(function (student){ %>
              <tr>
                <td><%= student.id %></td>
                <td><%= student.name %></td>
                <td><form method="post" action="/students/updateInterim/<%= student.id %>"> <button class="btn btn-primary" style="margin-left:23px;"name="Update"><i class="fa fa-pencil"></i></button></form></td>
                
              </tr>
              <% }) %>
            </tbody>
          </table>
        </div>
      </div>
      